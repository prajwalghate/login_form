extends layout

block content
    h1 Login
    p Please Login below
    form(method='post',action='/users/login',enctype='multipart/form-date')
        .form-group
            label Username
            input.form-control(name='username',type='text')
        .form-group
            label Password
            input.form-control(name='password',type='text')
        input.btn.btn-default(name='submit',type='submit',value='Login')